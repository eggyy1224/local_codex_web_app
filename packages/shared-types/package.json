    "test:unit": "vitest run --passWithNoTests",
    "test:integration": "vitest run --passWithNoTests",
    "test:e2e": "echo 'no e2e configured for shared-types'",
    "test:coverage": "vitest run --passWithNoTests --coverage.enabled=false"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}
